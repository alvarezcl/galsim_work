alias a='alias'
a k="clear ; ls -GtlFh "
a hm='cd ~'
a c='pushd '
a v='mv'
a u='cd ..'
a m='more'
a f='popd'
a sc='source ~/.bashrc'
a mggal='c ~/fizzAndAstro/lsst/luisAndMG_galsimwork_dir/mssgversion_code' 
a ecshrc='emacs ~/.bashrc'
a ebashrc='emacs ~/.bashrc'
